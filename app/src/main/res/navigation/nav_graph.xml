<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreen">

    <activity
        android:id="@+id/login"
        android:name="com.agendaSaudeOn.Login"
        android:label="activity_login"
        app:action="click"
        tools:layout="@layout/activity_login"
        app:startDestination="@id/action_login_to_agendaCuidador">
        <action
            android:id="@+id/action_login_to_agendaProfissional"
            app:destination="@id/agendaProfissional" />
        <action
            android:id="@+id/action_login_to_AgendaCuidador"
            app:destination="@id/AgendaCuidador" />
    </activity>
    <activity
        android:id="@+id/agendaProfissional"
        android:name="com.agendaSaudeOn.AgendaProfissionalPackage.AgendaProfissional"
        android:label="activity_agenda_profissional"
        tools:layout="@layout/activity_agenda_profissional" >
        <action
            android:id="@+id/action_agendaProfissional_to_Maps"
            app:destination="@id/Maps" />
    </activity>
    <activity
        android:id="@+id/splashScreen"
        android:name="com.agendaSaudeOn.SplashScreen"
        android:label="splash_screen"
        tools:layout="@layout/splash_screen" >
        <action
            android:id="@+id/action_splashScreen_to_login"
            app:destination="@id/login" />
    </activity>
    <fragment android:id="@+id/Maps" />
    <navigation
        android:id="@+id/AgendaCuidador"
        app:startDestination="@id/agendaCuidador" >
        <activity
            android:id="@+id/Procedimentos"
            android:name="com.agendaSaudeOn.AgendaCuidadorPackage.AgendaCuidador"
            android:label="activity_agenda_cuidador"
            tools:layout="@layout/activity_agenda_cuidador" >
            <action
                android:id="@+id/action_Procedimentos_to_Medicamentos"
                app:destination="@id/Medicamentos"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" />
        </activity>
        <fragment android:id="@+id/Sinais Vitais" >
            <action
                android:id="@+id/action_Sinais Vitais_to_Procedimentos"
                app:destination="@id/Procedimentos"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        </fragment>
        <fragment android:id="@+id/Medicamentos" />
    </navigation>
</navigation>